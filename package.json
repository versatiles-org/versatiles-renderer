{
	"name": "@versatiles/render",
	"version": "0.0.1",
	"description": "Renders vector tiles",
	"bin": {},
	"prefix": "./",
	"files": [
		"dist/**/*.js",
		"dist/**/*.d.ts"
	],
	"scripts": {
		"build-node": "rm -rf dist && tsc -p tsconfig.node.json && chmod +x dist/index.js",
		"build": "rm -rf release; npm run build-node",
		"check": "npm run lint && npm run test && npm run build",
		"lint": "eslint --color .",
		"prepack": "npm run build",
		"release": "npx vrt release-npm",
		"start": "tsx src/index.ts",
		"test-coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
		"test-typescript": "NODE_OPTIONS=--experimental-vm-modules jest",
		"test": "npm run test-typescript",
		"upgrade": "npm-check-updates -u && rm -f package-lock.json && rm -rf node_modules; npm i"
	},
	"type": "module",
	"author": "Michael Kreil <versatiles@michael-kreil.de>",
	"license": "Unlicense",
	"engines": {
		"node": ">= 22"
	},
	"dependencies": {
		"@mapbox/vector-tile": "^2.0.4",
		"@maplibre/maplibre-gl-style-spec": "^24.1.0",
		"@turf/union": "^7.2.0",
		"@versatiles/container": "^1.2.6",
		"@versatiles/style": "^5.7.0",
		"brace-expansion": "^4.0.1",
		"canvas": "^3.2.0",
		"commander": "^14.0.1",
		"pbf": "^4.0.1"
	},
	"devDependencies": {
		"@types/color": "^4.2.0",
		"@types/node": "^24.4.0",
		"@types/pbf": "^3.0.5",
		"@typescript-eslint/eslint-plugin": "^8.43.0",
		"@typescript-eslint/parser": "^8.43.0",
		"@versatiles/release-tool": "^2.4.3",
		"eslint": "^9.35.0",
		"npm-check-updates": "^18.1.1",
		"tsx": "^4.20.5",
		"typescript": "^5.9.2"
	}
}
