{
	"name": "@versatiles/render",
	"version": "0.0.1",
	"description": "Renders vector tiles",
	"bin": {},
	"prefix": "./",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "rm -rf dist && rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript && rm -rf dist/types",
		"check": "npm run format && npm run lint && npm run test && npm run build",
		"demo": "tsx src/demo.ts",
		"format": "prettier --write 'src/**/*.ts'",
		"lint": "eslint --color .",
		"prepack": "npm run build",
		"release": "npx vrt release-npm",
		"start": "tsx src/index.ts",
		"test-coverage": "vitest run --coverage",
		"test": "vitest run",
		"upgrade": "npm-check-updates -u -t minor; rm -f package-lock.json; rm -rf node_modules; npm i"
	},
	"type": "module",
	"author": "Michael Kreil <versatiles@michael-kreil.de>",
	"license": "Unlicense",
	"engines": {
		"node": ">= 22"
	},
	"dependencies": {
		"@mapbox/vector-tile": "^2.0.4",
		"@maplibre/maplibre-gl-style-spec": "^24.4.1",
		"@turf/union": "^7.3.4",
		"pbf": "^4.0.1"
	},
	"devDependencies": {
		"@eslint/js": "^10.0.1",
		"@rollup/plugin-node-resolve": "^16.0.3",
		"@rollup/plugin-typescript": "^12.3.0",
		"@types/node": "^25.2.3",
		"@types/pbf": "^3.0.5",
		"@versatiles/release-tool": "^2.7.1",
		"@versatiles/style": "^5.9.3",
		"@vitest/coverage-v8": "^4.0.18",
		"eslint": "^10.0.0",
		"geojson": "^0.5.0",
		"npm-check-updates": "^19.3.2",
		"prettier": "^3.8.1",
		"rollup": "^4.57.1",
		"rollup-plugin-dts": "^6.3.0",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.55.0",
		"vitest": "^4.0.18"
	}
}
